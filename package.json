{
  "name": "nodejs-service",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/index.ts",
    "pb": "npm run pb:compile && npm run pb:types",
    "pb:compile": "pbjs -t static-module -o ./src/pb/service.js service.proto",
    "pb:types": "pbts -o ./src/pb/service.d.ts ./src/pb/service.js"
  },
