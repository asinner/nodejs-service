  "license": "ISC",
  "description": "",
  "dependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/express": "^4.16.1",
    "@types/yargs": "^12.0.10",
    "body-parser": "^1.18.3",
    "class-validator": "^0.9.1",
    "express": "^4.16.4",
    "mysql2": "^1.6.5",
    "nodemon": "^1.18.10",
    "protobufjs": "^6.8.8",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^8.0.3",
    "typeorm": "^0.2.16",
    "typescript": "^3.3.3333",
    "yargs": "^13.2.2"
  }
}
